import org.openqa.selenium.WebDriver;
import org.openqa.selenium.edge.EdgeDriver;

public class Driver {
	public static void main(String[] args) {
		
		//PROVIDED THE LOCATION OF EDGE DRIVER
		System.setProperty("webdriver.edge.driver", "E:\\Selenium\\msedgedriver.exe");
		
		/*
		 * EdgeDriver() --> IT HELPS YOU TO EXECUTE SELENIUM SCRIPTS ON EDGE BROWSER.
		 */
		WebDriver driver = new EdgeDriver();
		
		/*
		 * 1. This method will launch a new browser and opens the given URL in the browser instance.
		 */
		driver.get("https://rahulshettyacademy.com/");
		
		/*
		 * 2. This method is used to retrieve the title of the web page the user currently working on.
		 */
		String title = driver.getTitle();
		System.out.println(title);
		
		String url = driver.getCurrentUrl();
		System.out.println(url);
		
		
		
		
		
		//WebDriver driver = new ChromeDriver();
		/*
		driver.close();
		//driver.findElement(null);
		//driver.findElements(null)
		driver.getCurrentUrl();
		driver.getPageSource();
		driver.getWindowHandle();
		driver.getWindowHandles();
		driver.manage();
		//driver.navigate()
		driver.quit();
		//driver.switchTo()*/
	}

}
